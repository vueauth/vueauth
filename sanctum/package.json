{
  "name": "@vueauth/sanctum",
  "version": "1.1.0-alpha.1",
  "scripts": {
    "build": "unbuild",
    "release": "pnpm build && bumpp --tag '@vueauth/sanctum@v%s' && pnpm publish"
  },
  "description": "A collection of composables for authenticating with Laravel Sanctum",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": "https://github.com/vueauth/vueauth",
  "author": "Luke Diebold",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "bumpp": "^10.1.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0"
  },
  "dependencies": {
    "@vue-composable/cookie": "^1.0.0-beta.24",
    "@vueauth/core": "^1.1.0-alpha.1",
    "@vueuse/core": "^8.2.3",
    "@vueuse/integrations": "^8.2.3",
    "@vueuse/shared": "^8.2.3",
    "deepmerge": "^4.2.2",
    "js-cookie": "^3.0.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  }
}