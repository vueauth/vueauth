{
  "name": "@vueauth/indexeddb",
  "version": "1.1.0-alpha.6",
  "scripts": {
    "release": "pnpm build && bumpp --tag '@vueauth/indexeddb@v%s' && pnpm publish",
    "build": "unbuild"
  },
