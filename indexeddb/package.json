      "require": "./dist/index.cjs"
    }
  },
  "repository": "https://github.com/vueauth/vueauth",
  "author": "Luke Diebold",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "bumpp": "^10.1.0"
  },
  "dependencies": {
    "@vueauth/core": "^1.1.0-alpha.*"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  }
}